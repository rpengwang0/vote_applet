@import '/components/auth/authUser.wxss';
@import '../activityRanking/activityRanking.wxss';

page {
  width: 100%;
  height: 100%;
  background:#f4f5f9;
}
.vote_main{padding:12rpx;}
.vote-container {
  width: 100%;
  height: 100%;
}
.vote-banner{background-color: #ccab69;padding:8rpx;}
.vote-banner , .vote-banner-img {
  /* width: 100%; */
  height: 55vw;
  display: block;
  position: relative;
}
.tag {
  margin: 0 16rpx 0 0;
  padding: 0rpx 8rpx;
  background: white;
  border-radius: 10rpx;
  font-size: 22rpx;
  border: 1px solid OrangeRed;
  color: OrangeRed;
}
.content_left {
  width: 530rpx !important;
  font-size: 26rpx;
}
.empty {
  border: 0 !important;
}
.vote-detail {
  width: 100%;
  height: 108rpx;
  color: black;
  position: relative;
  background:#fff;
  margin-top:10rpx;
  border-radius:8rpx;
  -webkit-border-radius:8rpx;
}
.y_vote_top ._ct{color:#ccab69;padding-top:16rpx;font-size: 42rpx;}
.vote-detail::after {
  content: '';
  width: 100%;
  height: 2rpx;
  background: #bbb;
  position: absolute;
  bottom: 0;
  left: 0;
  opacity: 0.5;
}

.vote-detail-item:not(:nth-last-child(1)) {
  position: relative;
}

.vote-detail-item:not(:nth-last-child(1))::after {
  content: '';
  width: 2rpx;
  height: 100%;
  background: #bbb;
  position: absolute;
  right: 0;
  top: 0;
  opacity: 0.5;
}

