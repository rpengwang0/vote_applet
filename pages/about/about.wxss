.homepage{
  position:fixed;
  height: 100%;
  width: 100%;
  background-color: #E6E6E6;
}
.header {
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color:white;
}
._blue{
  color: blue;
  text-decoration: underline;
  float: right
}
.about-v image{
  width: 265rpx;
}
.about-v{
  margin: 15px 0;
  display: flex;
  flex-direction: column;
  text-align: center;
  /* border-radius: 20%; */
}
.userinfo-nickname{
  margin-top: 25rpx;
  font-size: 32rpx;
  font-weight: bold;
}
.userinfo-version{
  font-size: 28rpx;
  color: #666;
}
.view-line{
  padding: 15px;
  font-size: 16px;
  border-bottom: 1px solid #E6E6E6;
}
.footer{
  position: absolute;
  bottom: 50px;
  background-color: #E6E6E6;
  text-align :center;/*垂直居中*/
  height: 30px;
  width: 100%;
  /* display:flex;
  flex-direction:column;
  justify-content: center;水平居中 */
}